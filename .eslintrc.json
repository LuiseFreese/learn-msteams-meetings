{
    "extends": [
        "standard"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "object-shorthand": 2,
        "quotes": "off",
        "semi": "off",
        "indent": "off",
        "space-before-function-paren": "off",
        "space-in-parens": "off",
        "padded-blocks": "off",
        "no-unused-vars": "off",
        "brace-style": "off",
        "no-trailing-spaces": "off",
        "eol-last": "off",
        "prefer-const": "off",
        "no-undef": "off",
        "react/jsx-key": "off",
        "no-use-before-define":"off"
    }
}